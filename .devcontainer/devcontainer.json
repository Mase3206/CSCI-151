{
	"name": "Fedora",  
	"image": "fedora:39",
	"onCreateCommand": "dnf install wget -y && wget \"https://raw.githubusercontent.com/Mase3206/hostthis/main/setup.sh\" && chmod +x setup.sh && ./setup.sh",
	"customizations": {
		"vscode": {
			"extensions": [
				"GitHub.vscode-pull-request-github",
				"eamodio.gitlens",
				"VisualStudioExptTeam.vscodeintellicode",
				"VisualStudioExptTeam.intellicode-api-usage-examples",
				"ms-python.vscode-pylance",
				"ms-python.python",
				"donjayamanne.python-environment-manager",
				"whosydd.snippet-creator",
				"ms-python.isort"
			]
		}
	}
}